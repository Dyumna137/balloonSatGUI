}

/* ===== TOOLTIP ===== */
QToolTip {
    background: #f2f2f2;
    border: 1px solid #999;
    color: #222;
    padding: 4px;
    border-radius: 3px;
}

/* ===== MENU BAR ===== */
QMenuBar {
    background: #e8e8e8;
