}

/* ===== SENSOR HEALTH INDICATORS ===== */
QLabel.health-dot {
    min-width: 20px;
    max-width: 20px;
    min-height: 20px;
    max-height: 20px;
    border-radius: 7px;
    margin: 0px 3px;
}

/* Green - Working */
QLabel#healthGreen {
    background-color: #4CAF50;
    border: none;
    min-width: 14px;
    max-width: 14px;
    min-height: 14px;
    max-height: 14px;
    border-radius: 7px;
}

/* Red - Error */
QLabel#healthRed {
    background-color: #f44336;
    border: none;
    min-width: 14px;
    max-width: 14px;
    min-height: 14px;
    max-height: 14px;
    border-radius: 7px;
}

/* Transparent - Inactive/Not Working */
QLabel#healthInactive {
    background-color: transparent;
    border: 1px solid #aaa;
    min-width: 14px;
    max-width: 14px;
    min-height: 14px;
    max-height: 14px;
    border-radius: 7px;
}

/* Sensor name labels */
QLabel.sensor-name {
    color: #555;
    font-size: 10pt;
    padding: 0px 3px;
}

/* ===== PROGRESS BARS (CPU/Memory) ===== */
QProgressBar {
