   <string notr="true">/* ============================================================
   GLOBAL UI THEME 
   ------------------------------------------------------------
   This theme keeps everything simple, modern, and consistent.
   Only well-chosen colors and spacing; no clutter.
   ============================================================ */


/* ===== ROOT WIDGET ===== */
QWidget {
    background-color: #f5f5f5;   /* soft grey background */
    color: #222;                 /* dark readable text */
    font-family: &quot;Segoe UI&quot;, Arial;
    font-size: 11pt;
}


/* ===== GROUP BOXES ===== */
QGroupBox {
    border: 1px solid #ccc;      /* subtle boundary */
    border-radius: 4px;
    margin-top: 8px;
    padding-top: 10px;
}

QGroupBox::title {
    color: #555;
    padding: 0 4px;
}


/* ===== TABLES ===== */
QTableWidget, QTableView {
    background: #fff;
    border: 1px solid #ccc;
    alternate-background-color: #f0f0f0;
    selection-background-color: #b7d5f0; 
    gridline-color: #ddd;
}

QHeaderView::section {
    background: #e6e6e6;
    border: 1px solid #ccc;
    padding: 4px;
}


/* ===== BUTTONS ===== */
QPushButton {
    background: #f2f2f2;
    border: 1px solid #bbb;
    border-radius: 4px;
    padding: 6px 12px;
}

QPushButton:hover { background: #e8e8e8; }
QPushButton:pressed { background: #dcdcdc; }
QPushButton:disabled {
    background: #eaeaea;
    color: #999;
}


/* ===== LABELS ===== */
QLabel {
    background: transparent;
    color: #222;
}

QLabel#titleLabel {
    font-weight: bold;
    color: #111;
}


/* ===== TEXT INPUTS ===== */
QLineEdit {
    background: #fff;
    border: 1px solid #ccc;
    border-radius: 3px;
    padding: 4px;
}

QLineEdit:focus {
    border-color: #4aa3ff;
}


/* ===== COMBO BOXES ===== */
QComboBox {
    background: #f2f2f2;
    border: 1px solid #bbb;
    border-radius: 3px;
    padding: 4px;
}

QComboBox:hover { background: #ebebeb; }
QComboBox QAbstractItemView {
    background: #fff;
    border: 1px solid #ccc;
    selection-background-color: #b7d5f0;
}


/* ===== SCROLLBARS ===== */
QScrollBar:vertical, QScrollBar:horizontal {
    background: #f0f0f0;
    border: none;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #bbb;
    border-radius: 5px;
}

QScrollBar::handle:hover { background: #999; }


/* ===== MENU &amp; STATUS BAR ===== */
QMenuBar {
    background: #e8e8e8;
    border-bottom: 1px solid #ccc;
}

QMenuBar::item:selected {
    background: #b7d5f0;
}

QMenu {
    background: #fff;
    border: 1px solid #ccc;
}

QMenu::item:selected {
    background: #b7d5f0;
}

QStatusBar {
    background: #efefef;
    border-top: 1px solid #ccc;
}


/* ============================================================
   STATUS LED INDICATORS â€” Clean Circle Style
   ------------------------------------------------------------
   These widgets override QLabel rules and stay perfectly round.
   Controlled using the dynamic &quot;status&quot; property:
       inactive / ok / warning / error
   ============================================================ */

StatusLED {
    all: unset;                      /* reset inherited QLabel styling */
    min-width: 18px;
    min-height: 18px;
    max-width: 18px;
    max-height: 18px;
    border-radius: 9px;              /* perfect circle */
    background-color: transparent;
    border: 1px solid #aaa;          /* inactive */
}

/* color states */
StatusLED[status=&quot;ok&quot;]       { background-color: #4CAF50; border: none; }
StatusLED[status=&quot;warning&quot;]  { background-color: #FFC107; border: none; }
StatusLED[status=&quot;error&quot;]    { background-color: #F44336; border: none; }
StatusLED[status=&quot;inactive&quot;] { background-color: transparent; border: 1px solid #aaa; }
</string>
