    max-width: 18px;
    max-height: 18px;

     border-radius: 9px;     /* circular shape */
     background-color: transparent; 
     border: 1px solid #aaa;  /* default inactive */
     margin: 0; 
     padding: 0;
}

/* ðŸŸ¢ OK */
StatusLED[status=&quot;ok&quot;], *[objectName$=&quot;Indicator&quot;][status=&quot;ok&quot;] {
    background-color: #4CAF50;
    border: none;
}

/* ðŸŸ  WARNING */
StatusLED[status=&quot;warning&quot;], *[objectName$=&quot;Indicator&quot;][status=&quot;warning&quot;] {
    background-color: #FFC107;
    border: none;
}

/* ðŸ”´ ERROR */
StatusLED[status=&quot;error&quot;], *[objectName$=&quot;Indicator&quot;][status=&quot;error&quot;] {
    background-color: #F44336;
    border: none;
}

/* âšª INACTIVE */
StatusLED[status=&quot;inactive&quot;], *[objectName$=&quot;Indicator&quot;][status=&quot;inactive&quot;] {
    background-color: transparent;
    border: 1px solid #aaa;
}


