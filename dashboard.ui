   <string notr="true">/* ============================================
   BALLOONSAT TELEMETRY DASHBOARD - LIGHT THEME
   Clean ‚Ä¢ Bright ‚Ä¢ Laboratory Style
   ============================================ */

/* Main Window - Light Background */
QMainWindow {
    background-color: #f5f5f5;
}

QWidget {
    background-color: #f5f5f5;
    color: #222;
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 11px;
}

/* ============================================
   GROUP BOXES
   ============================================ */

QGroupBox {
    background-color: transparent;
    border: 1px solid #cccccc;
    border-radius: 4px;
    margin-top: 8px;
    padding: 12px 8px 8px 8px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 8px;
    padding: 0 6px;
    color: #555;
    background-color: #f5f5f5;
    font-size: 11px;
    font-weight: 500;
}

/* ============================================
   BUTTONS
   ============================================ */

QPushButton {
    background-color: #e6e6e6;
    color: #222;
    border: 1px solid #b7b7b7;
    border-radius: 3px;
    padding: 8px 16px;
    font-size: 11px;
    font-weight: 500;
}

QPushButton:hover {
    background-color: #dadada;
    border: 1px solid #a8a8a8;
    color: #000;
}

QPushButton:pressed {
    background-color: #cfcfcf;
}

/* ============================================
   TABLES - Light, Clean
   ============================================ */

QTableWidget {
    background-color: #ffffff;
    alternate-background-color: #f2f2f2;
    border: 1px solid #cccccc;
    border-radius: 4px;
    gridline-color: #dddddd;
    color: #222;
    selection-background-color: #bee1ff;
    selection-color: #000;
    font-size: 11px;
}

QTableWidget::item {
    padding: 6px 10px;
    border: none;
}

QTableWidget::item:hover {
    background-color: #efefef;
}

QTableWidget::item:selected {
    background-color: #bee1ff;
}

/* Table Headers */
QHeaderView::section {
    background-color: #eaeaea;
    color: #444;
    border: none;
    border-bottom: 1px solid #cccccc;
    border-right: 1px solid #e0e0e0;
    padding: 6px 10px;
    font-size: 11px;
    font-weight: 600;
}

QHeaderView::section:hover {
    background-color: #dedede;
}

QHeaderView::section:first {
    background-color: #eaeaea;
}

/* ============================================
   PROGRESS BARS
   ============================================ */

QProgressBar {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 2px;
    text-align: center;
    color: #555;
    height: 18px;
    font-size: 10px;
    font-weight: 600;
}

QProgressBar::chunk {
    background-color: #0078d4;
    border-radius: 1px;
}

/* ============================================
   LABELS
   ============================================ */

QLabel {
    color: #333;
    background-color: transparent;
    border: none;
    font-size: 11px;
}

/* Sensor Name Labels */
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;BMP&quot;],
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;ESP32&quot;],
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;GPS&quot;],
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;MQ&quot;],
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;DHT&quot;],
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;MPU&quot;],
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;RTC&quot;] 
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;MAX6675&quot;]
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;LoRa&quot;]
QLabel[objectName*=&quot;lbl&quot;][objectName*=&quot;BMS&quot;]  {
    color: #222;
    font-size: 11px;
    font-weight: 500;
}

/* Status Dots */
QLabel[text=&quot;‚óè&quot;] {
    font-size: 16px;
}

/* CPU/Memory Labels */
QLabel[objectName*=&quot;CPU&quot;],
QLabel[objectName*=&quot;Mem&quot;] {
    color: #222;
    font-size: 11px;
}

/* ============================================
   SCROLLBARS
   ============================================ */

QScrollBar:vertical {
    background: #f5f5f5;
    width: 10px;
    border: none;
}

QScrollBar::handle:vertical {
    background: #cccccc;
    border-radius: 5px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: #bbbbbb;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    background: #f5f5f5;
    height: 10px;
    border: none;
}

QScrollBar::handle:horizontal {
    background: #cccccc;
    border-radius: 5px;
    min-width: 30px;
}

QScrollBar::handle:horizontal:hover {
    background: #bbbbbb;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    width: 0px;
}

/* ============================================
   STATUS BAR
   ============================================ */

QStatusBar {
    background-color: #e9e9e9;
    color: #666;
    border-top: 1px solid #cccccc;
    font-size: 9px;
    padding: 3px 10px;
}

/* ============================================
   MENU BAR
   ============================================ */

QMenuBar {
    background-color: #f0f0f0;
    color: #333;
    border-bottom: 1px solid #cccccc;
    padding: 2px;
    font-size: 11px;
}

QMenuBar::item {
    background-color: transparent;
    padding: 5px 10px;
    border-radius: 3px;
}

QMenuBar::item:selected {
    background-color: #dcdcdc;
    color: #000;
}

/* ============================================
   DROPDOWN MENUS
   ============================================ */

QMenu {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 4px;
    padding: 4px;
    color: #222;
    font-size: 11px;
}

QMenu::item {
    padding: 6px 20px;
    border-radius: 3px;
}

QMenu::item:selected {
    background-color: #e6e6e6;
}

QMenu::separator {
    height: 1px;
    background: #cccccc;
    margin: 4px 0;
}

/* ============================================
   SENSOR STATUS COLORS
   ============================================ */

/* Green - Healthy */
QLabel#lblBMPStatus,
QLabel#lblESP32Status,
QLabel#lblGPSStatus,
QLabel#lblMPU6050Status,
QLabel#lblDHT22Status,
QLabel#lblRTCStatus {
    color: #0b8f58;
    font-size: 16px;
}

/* Red - Error */
QLabel#lblMQ2Status,
QLabel#lblMQ7Status {
    color: #d7282f;
    font-size: 16px;
}

/* Orange - Warning */
QLabel#lblMQ131Status {
    color: #d98b00;
    font-size: 16px;
}

/* ============================================
   INPUT FIELDS
   ============================================ */

QLineEdit {
    background-color: #ffffff;
    color: #222;
    border: 1px solid #cccccc;
    border-radius: 3px;
    padding: 6px 10px;
    font-size: 11px;
}

QLineEdit:focus {
    border: 1px solid #0078d4;
}

/* ============================================
   COMBO BOXES
   ============================================ */

QComboBox {
    background-color: #ffffff;
    color: #222;
    border: 1px solid #cccccc;
    border-radius: 3px;
    padding: 6px 10px;
    font-size: 11px;
}

QComboBox:hover {
    border: 1px solid #b7b7b7;
}

QComboBox QAbstractItemView {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    selection-background-color: #d6eaff;
    color: #222;
}

/* ============================================
   CHECKBOXES
   ============================================ */

QCheckBox {
    color: #333;
    spacing: 8px;
    font-size: 11px;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #cccccc;
    border-radius: 3px;
    background-color: #ffffff;
}

QCheckBox::indicator:hover {
    border: 1px solid #bbbbbb;
}

QCheckBox::indicator:checked {
    background-color: #0078d4;
    border: 1px solid #0078d4;
}

/* ============================================
   TOOLTIPS
   ============================================ */

QToolTip {
    background-color: #ffffff;
    color: #333;
    border: 1px solid #cccccc;
    border-radius: 3px;
    padding: 6px 10px;
    font-size: 10px;
}

/* ============================================
   PLOT AREA
   ============================================ */

QWidget#trajectoryPlot,
QWidget#altitudePlot,
QWidget#cameraPreview {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    border-radius: 4px;
